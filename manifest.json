{
    "name":"mirage",
    "version":"0.11.0",
    "icons":{
        "16":"site/assets/images/icon_16.png",
        "32":"site/assets/images/icon_32.png",
        "48":"site/assets/images/icon_48.png",
        "128":"site/assets/images/icon_128.png"
    },
    "background": {
        "scripts": [ "chrome-specific/background.js" ]
    },
    "web_accessible_resources": [
        "site/index.html"
